<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ux_color_white"
    android:orientation="vertical"
    android:minHeight="100dp">
    
     <TextView
        android:id="@+id/rv_security_dsg_verify_title"
        style="@style/fx_title_style"
        android:text="@string/rv_security_dsg_verify_title" />

    <TextView
        android:id="@+id/rv_security_dsg_verify_result"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="5dp"
        android:gravity="left"
        android:paddingLeft="5dp"
        android:textColor="#F000"
        android:textSize="13.3dp"/>

</LinearLayout>